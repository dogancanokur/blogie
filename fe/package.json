{
  "name": "vite-react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && vite build",
    "build:turbo": "npx turbo build",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc",
    "check:turbo": "npx turbo lint type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^5.0.17",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.289.0",
    "million": "2.6.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.17.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-helmet": "^6.1.8",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "commitlint": "^18.2.0",
    "eslint": "^8.52.0",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  }
}
